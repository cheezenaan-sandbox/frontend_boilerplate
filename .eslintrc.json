{
  "env": {
    "browser": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parser": "typescript-eslint-parser",
  "plugins": ["typescript", "prettier"],
  "rules": {
    "no-undef": "off",
    "import/extensions": ["error", { "ts": "never" }],
    "import/no-unresolved": "off",
    "prettier/prettier": "error"
  }
}
